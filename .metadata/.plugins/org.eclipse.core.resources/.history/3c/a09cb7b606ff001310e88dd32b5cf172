import random
__author__ = 'Alan Tai'


import webapp2
import string
from flask import Flask
from simplekv.memory import DictStore
from flaskext.kvsession import KVSessionExtension

APPLICATION_NAME = 'Google+ Demo'
app_flask = Flask(__name__)
app_flask.secret_key = ''.join(random.choice(string.ascii_uppercase + string.digits) for x in xrange(32))




class GooglePlusIndex(webapp2.RequestHandler):
    def get(self):
        pass

class GooglePlusConnect(webapp2.RequestHandler):
    def post(self):
        pass
    
class GooglePlusDisconnect(webapp2.RequestHandler):
    def post(self):
        pass
    
class GooglePlusPeople(webapp2.RequestHandler):
    def get(self):
        pass
    
    
app = webapp2.WSGIApplication([('/google_plus_index', GooglePlusIndex),
                               ('/google_plus_connect', GooglePlusConnect),
                               ('/google_plus_disconnect', GooglePlusDisconnect),
                               ('/google_plus_people', GooglePlusPeople)], debug = True)