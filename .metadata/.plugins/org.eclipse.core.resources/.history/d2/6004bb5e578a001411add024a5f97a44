from handlers.handler_webapp2_extra_auth import BaseHandler
from libs_crawler import feedparser

class CrawlingDispatcher(BaseHandler):
    def get(self):
        self._read_feed()
    
    def _read_feed(self):
        feeds = feedparser.parse("http://www.wine-searcher.com/")